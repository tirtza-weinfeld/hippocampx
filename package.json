{
  "name": "hippocampx",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": " next dev --turbopack ",
    "build": "pnpm build:plugins && next build --turbopack",
    "build:scripts": "pnpm problems:generate",
    "start": "next start",
    "lint": "eslint",
    "lint:full": "tsc --noEmit && tsc -p plugins/tsconfig.json --noEmit && eslint",
    "lint:fix": "eslint --fix",
    "build:plugins": "tsc -p plugins/tsconfig.json",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed:collections": "tsx lib/db/seeds/scripts/seed-collections.ts",
    "db:seed:songs": "tsx lib/db/seeds/scripts/seed-songs.ts",
    "db:seed:dictionary": "tsx lib/db/seeds/scripts/seed-dictionary-v2.ts",
    "db:run": "tsx scripts/run-sql.ts",
    "db:embed:senses": "tsx scripts/embed-senses.ts",
    "db:discover:relations": "tsx scripts/discover-relations.ts",
    "db:pipeline": "tsx scripts/seed-pipeline.ts",
    "problems:create": "python3 backend/scripts/problems/create_problem.py",
    "problems:extract-lsp-index": "python3 backend/scripts/problems/generate_lsp_index.py --root backend/algorithms --out lib/extracted-metadata/lsp_index.json",
    "problems:extract-symbol-tags": "python3 backend/scripts/problems/generate_symbol_tags.py --root backend/algorithms --out lib/extracted-metadata/symbol_tags.json",
    "problems:extract-uses": "python3 backend/scripts/problems/generate_uses.py --root backend/algorithms --out lib/extracted-metadata/uses.json",
    "problems:extract-expressions": "python3 backend/scripts/problems/generate_expressions.py --root backend/algorithms --out lib/extracted-metadata/expressions.json",
    "problems:extract-comments-inline": "python3 backend/scripts/problems/generate_comments_inline.py --root backend/algorithms --out lib/extracted-metadata/comments-inline.json --out-symbols lib/extracted-metadata/comments-inline-symbols.json",
    "problems:extract-metadata": "python3 backend/scripts/problems/extract_problems_metadata.py",
    "problems:sync-db": "python3 backend/scripts/problems/sync_problems_to_db.py",
    "problems:transform-symbol-tags-field-to-lists": "python3 backend/scripts/problems/transform-field-to-lists.py --input lib/extracted-metadata/symbol_tags.json --output lib/extracted-metadata/symbol_tags.json --fields intuition topics ",
    "problems:generate-tooltips-metadata": "pnpm problems:extract-lsp-index &&  pnpm problems:extract-symbol-tags && pnpm problems:extract-uses && pnpm problems:extract-expressions && pnpm problems:extract-comments-inline && pnpm problems:transform-symbol-tags-field-to-lists",
    "problems:generate-mdx": "pnpm problems:extract-metadata  && tsx scripts/generate-problem-mdx.ts",
    "problems:generate-mdx:single": "tsx scripts/generate-problem-mdx.ts --problem",
    "problems:generate": "pnpm problems:generate-tooltips-metadata  && pnpm problems:generate-mdx",
    "problems:generate:single": "f() { python3 backend/scripts/problems/extract_problems_metadata.py --problem \"$1\" && tsx scripts/generate-problem-mdx.ts --problem \"$1\"; }; f",
    "problems:single": "f() { python3 backend/scripts/problems/generate_lsp_index.py --root backend/algorithms --out lib/extracted-metadata/lsp_index.json --problem \"$1\" && python3 backend/scripts/problems/generate_symbol_tags.py --root backend/algorithms --out lib/extracted-metadata/symbol_tags.json --problem \"$1\" && python3 backend/scripts/problems/generate_uses.py --root backend/algorithms --out lib/extracted-metadata/uses.json --problem \"$1\" && python3 backend/scripts/problems/generate_expressions.py --root backend/algorithms --out lib/extracted-metadata/expressions.json --problem \"$1\" && python3 backend/scripts/problems/generate_comments_inline.py --root backend/algorithms --out lib/extracted-metadata/comments-inline.json --out-symbols lib/extracted-metadata/comments-inline-symbols.json --problem \"$1\" && python3 backend/scripts/problems/transform-field-to-lists.py --input lib/extracted-metadata/symbol_tags.json --output lib/extracted-metadata/symbol_tags.json --fields intuition topics && python3 backend/scripts/problems/extract_problems_metadata.py --problem \"$1\" && tsx scripts/generate-problem-mdx.ts --problem \"$1\" && python3 backend/scripts/problems/sync_problems_to_db.py \"$1\"; }; f",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:browser": "vitest --config vitest.browser.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "pnpm test:run && pnpm test:e2e",
    "test:py": "cd backend && pytest scripts/test_extract_metadata.py -v",
    "commit": "tsx scripts/commit.ts"
  },
